<h3>Leader Board</h3>

<table>
  <thead>
    <tr>
      <th>User</th>
      <td></td>
      <th>Wins</th>
      <td></td>
      <th>Total</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td></td>
        <td><%= user.wins %></td>
        <td></td>
        <td><%= user.total %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>